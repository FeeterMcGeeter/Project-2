<div class="wrapper">
    <!-- {{! ===== HEADER ===== }} -->
    <div class="section no-pad-bot" id="index-banner">
        <div class="container">
            <img class="coffee" src="img/coffeetalk.png">
            <nav>
                <div class="nav-wrapper">
                    <a href="#" class="brand-logo"><span class="member-name"></span></a>
                    <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                    <ul id="nav-mobile" class="right hide-on-med-and-down">
                        <li class="home"><a href="#">Home</a></li>
                        <li class="profile"><a href="#">Profile</a></li>
                        <li class="logOut"><a href="#">Log Out</a></li>
                    </ul>
                </div>
            </nav>

            <!-- ===== HAMBURGER MENU =====  -->
            <ul class="sidenav" id="mobile-demo">
                <li class="home"><a href="#">Home</a></li>
                <li class="profile"><a href="#">Profile</a></li>
                <li class="logOut"><a href="#">Log Out</a></li>
            </ul>
        </div>
    </div>

    <div class="container">
        <div class="section">
            <div class="row">
                <div class="col s6">
                    <div class="liked-messages">
                        <h3>Liked Messages</h3>
                        <div class="liked-box" style="border: 1px solid #000;">
                            {{#each liked}}
                            <div class="message-liked">
                                <h5>{{this.text}}</h5>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

